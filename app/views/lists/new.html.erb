<div class="container">
  
      <h1>Add a list </h1>
      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit "Submit", class: "btn btn-primary" %>
      <% end %>
      <%= link_to "Back to Your List", lists_path %>
</div>